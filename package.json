{
  "name": "milo-dog",
  "version": "0.0.1",
  "description": "A massively over-engineered website about a small sausage dog, Milo.",
  "author": "Nico Castro",
  "license": "MIT",
  "scripts": {
    "run:development:api": "docker-compose -f 'dev.docker-compose.yml' run api",
    "run:production:api": "docker-compose -f 'prod.docker-compose.yml' run api",
    "serve": "yarn start:production",
    "start": "yarn start:development",
    "start:development": "docker-compose -f 'dev.docker-compose.yml' up",
    "start:production": "docker-compose -f 'prod.docker-compose.yml' up",
    "test": "yarn test:api",
    "test:api": "cd api && yarn test"
  }
}
